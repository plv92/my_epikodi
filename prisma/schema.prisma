generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Track {
  id        Int      @id @default(autoincrement())
  title     String
  artist    String
  album     String?
  url       String
  createdAt DateTime @default(now())
  cover     String?   // cover URL
  genre     String?   // genre
  year      Int?      // ann√©e de sortie
}
